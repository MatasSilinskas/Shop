@model WEB.Models.ListOfRatings

@{
    ViewBag.Title = "StoreList";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}
@using (Html.BeginForm())
{
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <form class="form-inline my-2 my-lg-0 mr-lg-2">
                @Html.TextBoxFor(x => x.shopToRate)
                @if (Model != null)
                {
                    <table>
                        @for (int i = 0; i < Model.list.Count; i++)
                        {
                            @:
                            <tr></tr>
                            <td>
                                @Html.CheckBoxFor(model => model.list[i].isChecked)
                                <lable>@Model.list[i].rating</lable>
                            </td>
                        }
                    </table>
                    <input id="Submit1" type="submit" value="submit rating" />
                }
                @if (ViewBag.rezult != null)
                {
                    <h5>@ViewBag.rezult</h5>
                }
            </form>
        </li>
    </ol>

}
<div>
    @Html.ActionLink("Back to Settings", "Index")
</div>